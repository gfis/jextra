<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<grammar>
<sym>EOF</sym>
<sym>IDENTIFIER</sym>
<sym>NUMBER</sym>
<sym>STRING</sym>
<sym>;</sym>
<comment>	Aho and Ullman [1972] give an example (7.27) </comment>
<comment>  for a grammar which is LR(1) but not LALR(1) </comment>
<rule>
<left>axiom</left>
<prod>
<sym>S</sym>
</prod>
</rule>
<rule>
<left>S</left>
<prod>
<sym>A</sym>
<sym>a</sym>
</prod>
<prod>
<sym>B</sym>
<sym>b</sym>
</prod>
<prod>
<sym>d</sym>
<sym>A</sym>
<sym>b</sym>
</prod>
<prod>
<sym>d</sym>
<sym>B</sym>
<sym>a</sym>
</prod>
</rule>
<rule>
<left>A</left>
<prod>
<sym>c</sym>
</prod>
</rule>
<rule>
<left>B</left>
<prod>
<sym>c</sym>
</prod>
</rule>
</grammar>
